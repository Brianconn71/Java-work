package week8Lambdas;

interface Printable{
	String print(String t);
}

public class Lambasexample {

	public static void main(String[] args) {
		Cat mycat = new Cat();
		
		Printable y = (t) -> "hi" + t;
		
		printThing(y);

	}
	
	static void printThing(Printable thing) {
		thing.print(" Hi");
	}

}
