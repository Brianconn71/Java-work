package week8Lambdas;

interface Printable{
	void print(String t);
}

public class Lambasexample {

	public static void main(String[] args) {
		Cat mycat = new Cat();
		
		Printable y = t -> System.out.println("Meow" + t);
		
		printThing(y);

	}
	
	static void printThing(Printable thing) {
		thing.print(" Hi");
	}

}
